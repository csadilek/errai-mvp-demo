		<!-- BEGIN JBOSS AS 7 PROFILE -->

		<profile>
			<id>jboss7</id>
			<properties>
				<webDescriptor>jboss</webDescriptor>
			</properties>

			<dependencies>
				<!-- All errai package are installed with the service archive -->
				<dependency>
					<groupId>org.jboss.errai</groupId>
					<artifactId>errai-bus</artifactId>
					<exclusions>

						<exclusion>
							<groupId>junit</groupId>
