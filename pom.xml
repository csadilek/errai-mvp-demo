			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-antrun-plugin</artifactId>
						<version>1.2</version>
						<executions>
							<execution>
								<id>jboss.package</id>
								<phase>prepare-package</phase>
								<configuration>
									<tasks>
										<copy file="src/jboss/web.xml" toDir="src/main/webapp/WEB-INF"
