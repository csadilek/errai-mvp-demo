					<groupId>org.mortbay.jetty</groupId>
					<artifactId>jetty-naming</artifactId>
					<version>${jetty.version}</version>
					<scope>provided</scope>
				</dependency>

				<dependency>
					<groupId>org.jboss.errai</groupId>
					<artifactId>errai-cdi-jetty</artifactId>
					<version>${errai.version}</version>
					<scope>provided</scope>
				</dependency>

			</dependencies>
			<build>
				<plugins>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-antrun-plugin</artifactId>
						<executions>
							<execution>
								<id>jetty.package</id>
								<phase>prepare-package</phase>
								<configuration>
									<tasks>
										<copy file="src/jetty7+/web.xml" toDir="src/main/webapp/WEB-INF"
											overwrite="true" />
										<!-- We don't need jetty-env as we are using the BeanManagerResourceBindingListener 
											to bind the BeanManager in JNDI. -->
										<delete file="src/main/webapp/WEB-INF/classes/jetty-env.properties" />
										<delete file="src/main/webapp/WEB-INF/classes/jndi.properties" />
									</tasks>
								</configuration>
								<goals>
									<goal>run</goal>
								</goals>
							</execution>
						</executions>
					</plugin>
				</plugins>
			</build>
		</profile>

